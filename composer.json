{
    "name": "friendsofsymfony/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony FOSUserBundle",
    "keywords": [
        "User management"
    ],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Thibault Duplessis"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/friendsofsymfony/FOSUserBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "doctrine/persistence": "^1.3",
        "symfony/config": "^3.4 || ^4.4",
        "symfony/form": "^3.4 || ^4.4",
        "symfony/framework-bundle": "^3.4 || ^4.4",
        "symfony/security-bundle": "^3.4 || ^4.4",
        "symfony/translation": "^3.4 || ^4.4",
        "symfony/twig-bundle": "^3.4 || ^4.4",
        "symfony/validator": "^3.4 || ^4.4",
        "twig/twig": "^2.0 || ^3.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "symfony/doctrine-bridge": "<2.7"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^7.0 ||  ^8.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/cache": "^3.4 || ^4.4",
        "symfony/console": "^3.4 || ^4.4",
        "symfony/phpunit-bridge": "^3.4 || ^4.4",
        "symfony/yaml": "^3.4 || ^4.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "FOS\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\UserBundle\\Tests\\": "/Tests/"
        }
    },
    "support": {
        "docs": "https://symfony.com/doc/master/bundles/FOSUserBundle/index.html"
    }
}
